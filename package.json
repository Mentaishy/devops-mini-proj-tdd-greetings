{
    "name": "projet_devops",
    "version": "1.0.0",
    "description": "devops node project initial setup",
    "main": "index.js",
    "scripts": {
        "start": "nodemon -e '*' ./bin/www",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "code:fix": "prettier --write . && eslint --fix .",
        "commit": "cz",
        "test": "jest --coverage",
        "test:watch": "jest --watchAll",
        "build": "webpack ./src/app.js -o public/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Mentaishy/devops-mini-proj-tdd-greetings.git"
    },
    "keywords": [
        "project_Devops"
    ],
    "author": "marwan",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Mentaishy/devops-mini-proj-tdd-greetings/issues"
    },
    "homepage": "https://github.com/Mentaishy/devops-mini-proj-tdd-greetings#readme",
    "devDependencies": {
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.53.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-n": "^16.3.1",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "prettier": "3.1.0",
        "supertest": "^6.3.3"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "webpack-cli": "^5.1.4"
    }
